{
  "name": "testcafe-sample",
  "version": "0.1.0",
  "description": "A sample of TestCafe for executing tests locally and on Sauce Labs",
  "main": "index.js",
  "scripts": {
    "test.local.chrome": "testcafe chrome tests/ -c 6",
    "test.sauce.chrome": "testcafe saucelabs:chrome tests/ -c 6",
    "test.sauce.all": "testcafe saucelabs:chrome,saucelabs:safari,saucelabs:firefox,saucelabs:MicrosoftEdge,saucelabs:'internet explorer' tests/ -c 6",
    "lint": "eslint . --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wswebcreation/testcafe-sample.git"
  },
  "keywords": [
    "testcafe",
    "sauce",
    "labs",
    "automation"
  ],
  "author": "wswebcreation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wswebcreation/testcafe-sample/issues"
  },
  "homepage": "https://github.com/wswebcreation/testcafe-sample#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-testcafe": "^0.2.1",
    "testcafe": "^1.20.1",
    "testcafe-browser-provider-saucelabs": "^1.7.0"
  }
}
